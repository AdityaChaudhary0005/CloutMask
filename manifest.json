{
  "name": "CloutMask",
  "version": "1.1.0",
  "manifest_version": 3,
  "description": "Browse BitClout as any user",
  "author": "Paul Burke",
  "homepage_url": "https://github.com/iPaulPro/CloutMask",
  "content_scripts": [
    {
      "matches": [
        "https://bitclout.com/*"
      ],
      "js": [
        "main.js"
      ],
      "css": [
        "main.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "images/*"
      ],
      "matches": [
        "https://bitclout.com/*"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "/images/icon_16.png",
    "32": "/images/icon_32.png",
    "48": "/images/icon_48.png",
    "128": "/images/icon_128.png"
  }
}
